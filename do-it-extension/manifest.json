{
  "name": "DO IT!",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "For when you need motivation! Shia LaBeouf delivers the most intense motivational speech of all-time IN YOUR BROWSER! ",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "tabs", "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
     "default_icon": {
        "19": "icons/icon19.png",
        "38": "icons/icon38.png"
      },
    "default_title": "DO IT"
  },
  "web_accessible_resources": [
    "media/*"
  ]
}